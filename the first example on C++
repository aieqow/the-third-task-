#include <iostream>
#include <queue>
#include <vector>
using namespace std;

int main() {
    // Макс-куча (по умолчанию)
    priority_queue<int> maxHeap;
    
    // Мини-куча
    priority_queue<int, vector<int>, greater<int>> minHeap;
    
    // Добавление элементов в мини-кучу
    minHeap.push(5);
    minHeap.push(2);
    minHeap.push(8);
    minHeap.push(1);
    
    // Извлечение минимального элемента
    while (!minHeap.empty()) {
        cout << minHeap.top() << " "; // 1 2 5 8
        minHeap.pop();
    }
    
    return 0;
}

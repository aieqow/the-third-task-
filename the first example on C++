#include <iostream>
#include <algorithm>
#include <vector>

int main() {
    std::vector<int> vec = {1, 3, 5, 7, 9};  // Отсортированный вектор
    int target = 5;                           // Искомый элемент
    
    bool found = std::binary_search(vec.begin(), vec.end(), target);  // Встроенный поиск
    
    std::cout << (found ? "Найдено" : "Не найдено") << std::endl;
    return 0;
}

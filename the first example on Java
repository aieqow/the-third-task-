import java.util.Arrays;

public class BinarySearch {
    public static void main(String[] args) {
        int[] arr = {1, 3, 5, 7, 9};  // Отсортированный массив
        int target = 5;                // Искомый элемент
        
        int result = Arrays.binarySearch(arr, target);  // Встроенный бинарный поиск
        
        System.out.println(result >= 0 ? "Найдено: " + result : "Не найдено");
    }
}

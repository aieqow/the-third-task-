import java.util.PriorityQueue;

public class Main {
    public static void main(String[] args) {
        PriorityQueue<Integer> heap = new PriorityQueue<>(); // создаем минимальную кучу
        heap.add(5); // добавляем элемент
        heap.add(3);
        heap.add(8);
        System.out.println(heap.poll()); // извлекаем и удаляем минимальный элемент (3)
    }
}
